{
  "name": "markify",
  "displayName": "Markdownify webinfo",
  "version": "0.1.0",
  "description": "aaa",
  "author": "Lanqing Huang <lqhuang@outlook.com>",
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  },
  "type": "module",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "test": "vitest",
    "lint": "eslint --ext .ts,.js src --fix",
    "typecheck": "tsc --noEmit",
    "web-ext:start": "web-ext run --source-dir ./dist ",
    "web-ext:build": "web-ext build -o --source-dir ./dist --filename markdownify-metadata-v0.0.1.xpi"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "cheerio": "^1.0.0",
    "es-toolkit": "^1.17.0",
    "plasmo": "0.89.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regex": "^4.1.3"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.12.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/chrome": "^0.0.270",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "hasown": "npm:@nolyfill/hasown@latest",
      "isarray": "npm:@nolyfill/isarray@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "typedarray": "npm:@nolyfill/typedarray@latest"
    }
  }
}
